{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "the bing",
  "authors": [],
  "contact": {},
  "license": "GPLv3",
  "icon": "assets/molecraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.kaboomroads.molecraft.client.data.MolecraftDataGenerator"
    ],
    "client": [
      "com.kaboomroads.molecraft.client.MolecraftClient"
    ],
    "main": [
      "com.kaboomroads.molecraft.Molecraft"
    ]
  },
  "accessWidener": "${modid}.accesswidener",
  "mixins": [
    "${modid}.mixins.json",
    {
      "config": "${modid}.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
